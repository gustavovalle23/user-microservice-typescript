{
	"moduleFileExtensions": ["js", "ts"],
	"moduleNameMapper": {
	  "^@/test/(.*)$": "<rootDir>/test/$1",
	  "^@/(.*)$": "<rootDir>/src/$1"
	},
	"rootDir": "..",
	"testRegex": "spec.ts$",
	"transform": {
	  "^.+\\.(t|j)s$": "ts-jest"
	},
	"collectCoverageFrom": [
	  "src/**/**",
	  "!src/**/index.ts",
	  "!src/**/main.ts",
	  "!src/**/*.snap",
	  "!**/.eslintrc.js"
	]
  }

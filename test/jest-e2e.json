{
	"moduleFileExtensions": ["js", "ts"],
	"moduleNameMapper": {
	  "^@/test/(.*)$": "<rootDir>/test/$1",
	  "^@/(.*)$": "<rootDir>/src/$1"
	},
	"coverageDirectory": "./test/coverage",
	"rootDir": "..",
	"testRegex": ".e2e-spec.ts$",
	"transform": {
	  "^.+\\.(t|j)s$": "ts-jest"
	},
	"collectCoverageFrom": [
	  "src/**/**",
	  "!src/**/index.ts",
	  "!src/**/main.ts",
	  "!src/common/env.helper.ts"
	]
  }
